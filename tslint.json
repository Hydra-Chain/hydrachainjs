{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-var-requires": false,
        "semicolon": [
            true,
            "never"
        ],
        "trailing-comma": [
            true
        ],
        "object-literal-sort-keys": false,
        "variable-name": [
            true,
            "allow-leading-underscore"
        ],
        "ordered-imports": false
    },
    "rulesDirectory": []
}
